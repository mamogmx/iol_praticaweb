        <element id="hw_no_barriere_architettoniche" title="Nascondi quando no barriere architettoniche (intervento non soggetto art.82 e seguenti dpr 380/2011)" type="PlominoHidewhen">
          <id type="Products.Archetypes.Field.StringField">hw_no_barriere_architettoniche</id>
          <Formula type="Products.Archetypes.Field.TextField"><![CDATA[return not context.getItem('barriere_architettoniche_opt','1') in ['2','4']]]></Formula>
          <isDynamicHidewhen type="Products.Archetypes.Field.BooleanField">False</isDynamicHidewhen>
        </element>
        <element id="hw_no_deroga_barriere_architettoniche" title="Nascondi quando non è presente la deroga alle barriere architettoniche" type="PlominoHidewhen">
          <id type="Products.Archetypes.Field.StringField">hw_no_deroga_barriere_architettoniche</id>
          <Formula type="Products.Archetypes.Field.TextField"><![CDATA[return context.getItem('barriere_architettoniche_deroga_opt','1')=='1']]></Formula>
