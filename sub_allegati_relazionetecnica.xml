          <isDynamicHidewhen type="Products.Archetypes.Field.BooleanField">False</isDynamicHidewhen>
        </element>
        <element id="hw_consumi_energetici" title="Nascondi quando interessato da consumi energetici" type="PlominoHidewhen">
          <id type="Products.Archetypes.Field.StringField">hw_consumi_energetici</id>
          <Formula type="Products.Archetypes.Field.TextField"><![CDATA[return not (context.getItem('risparmio_energetico_opt','1')=='1' and context.getItem('fonti_rinnovabili_opt','1')=='1')]]></Formula>
          <isDynamicHidewhen type="Products.Archetypes.Field.BooleanField">False</isDynamicHidewhen>
