        <element id="pw_riferimento_normativa" title="Normativa di riferimento" type="PlominoField">
          <id type="Products.Archetypes.Field.StringField">pw_riferimento_normativa</id>
          <FieldType type="Products.Archetypes.Field.StringField">TEXT</FieldType>
          <FieldMode type="Products.Archetypes.Field.StringField">COMPUTEDONSAVE</FieldMode>
          <Formula type="Products.Archetypes.Field.TextField"><![CDATA[app = context.getItem('iol_tipo_app','')
sportello = context.getItem('iol_tipo_proc','')
if app == 'regolarizzazione':
    tipo = context.getItem('comunicazione_opt','')
elif app == 'cila':
    tipo = context.getItem('qualifica_interventi_opt','')
else:
    tipo = context.getItem('qualificazione_intervento','')

    
categoria = dict(
    scia = { '1' : '300', '4' : '301', '5' : '302', '2' : '302', '3' : '303'},
    cila = { '1' : '400', '2' : '401', '3' : '401', '4' : '402', '5' : '403', '6' : '403', '7' : '404'},
    dia = { '1' : '103', '2' : '104', '3' : '104', '4' : '105', '5' : '100', '6' : '101', '7' : '106'},
    pdc = { '1' : '202', '2' : '200', '3' : '204', '4' : '206', '5' : '205'},
    regolarizzazione = {'1' : '900', '2' : '901'},
    default = '0'
)    

if app in categoria.keys():
    if tipo in categoria[app].keys():
        return categoria[app][tipo]
    else:
        return categoria['default']
else:
    return categoria['default']]]></Formula>
