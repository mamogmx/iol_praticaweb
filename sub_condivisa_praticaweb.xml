        <element id="pw_tipopratica" title="Tipo Pratica" type="PlominoField">
          <id type="Products.Archetypes.Field.StringField">pw_tipopratica</id>
          <FieldType type="Products.Archetypes.Field.StringField">TEXT</FieldType>
          <FieldMode type="Products.Archetypes.Field.StringField">COMPUTEDONSAVE</FieldMode>
          <Formula type="Products.Archetypes.Field.TextField"><![CDATA[app = context.getItem('iol_tipo_app','')
sportello = context.getItem('iol_tipo_proc','')
tipo = context.getItem('qualificazione_intervento','')
if app == 'regolarizzazione':
    return '32000'
elif app == 'pdc':
    return '11000'
elif app == 'dia':
    if sportello == 'suap':
        return '10500'     # SUAP
    else:
        if tipo == '7':        # Variante
            return '10100'
        elif tipo == '3':      # DIA subordinata a Parere Ambientale
            return '10050'
        else:
            return '10000'
elif app == 'cila':
    if '2' in context.getItem('atti_assenso_opt',[]):
        if sportello == 'sue':
            return '21310'
        else:
            return '21410'
    else:
        if sportello == 'sue':
            return '21300'
        else:
            return '21400'
elif app == 'scia':
    if context.getItem('atti_assenso_opt',[]) in ['2','4']:
        if sportello == 'sue':
            return '21110'
        else:
            return '21210'
    else:
        if sportello == 'sue':
            return '21100'
        else:
            return '21200']]></Formula>
