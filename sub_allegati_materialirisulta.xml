        <element id="hw_no_materiali_risulta" title="Nascondi quando nessun materiale di risulta" type="PlominoHidewhen">
          <id type="Products.Archetypes.Field.StringField">hw_no_materiali_risulta</id>
          <Formula type="Products.Archetypes.Field.TextField"><![CDATA[return not plominoDocument.getItem('materiali_risulta_opt','1') == '1']]></Formula>
          <isDynamicHidewhen type="Products.Archetypes.Field.BooleanField">False</isDynamicHidewhen>
        </element>
        <element id="hw_no_provvedi_via" title="Nascondi quando opere non soggette a VIA o AIA (art. 182-bis,comma 2-bis, d.lgs 152/2006 e d.m. 161/2012)" type="PlominoHidewhen">
          <id type="Products.Archetypes.Field.StringField">hw_no_provvedi_via</id>
          <Formula type="Products.Archetypes.Field.TextField"><![CDATA[return context.getItem('materiali_risulta_limite_volume','0')!='3']]></Formula>
          <isDynamicHidewhen type="Products.Archetypes.Field.BooleanField">False</isDynamicHidewhen>
        </element>
        <element id="hw_no_autocert_titolare_arpa" title="Nascondi quando soggette a VIA o ARPA o mc inferiori a 6000 (art. 41-bis, comma 1, d.l. 69/2013) " type="PlominoHidewhen">
          <id type="Products.Archetypes.Field.StringField">hw_no_autocert_titolare_arpa</id>
          <Formula type="Products.Archetypes.Field.TextField"><![CDATA[return context.getItem('materiali_risulta_limite_volume','0')!='1']]></Formula>
          <isDynamicHidewhen type="Products.Archetypes.Field.BooleanField">False</isDynamicHidewhen>
        </element>
        <element id="hw_no_autocert_scavo_rocce" title="Nascondi quando non riutilizzate nello stesso luogo (art. 185, comma 1, lettera c, d.lgs 152/2006)" type="PlominoHidewhen">
          <id type="Products.Archetypes.Field.StringField">hw_no_autocert_scavo_rocce</id>
          <Formula type="Products.Archetypes.Field.TextField"><![CDATA[return context.getItem('materiali_risulta_opt','0')!='3']]></Formula>
